{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "consistent-return": 0,
        "no-plusplus": 0,
        "no-param-reassign": 0
    },
    "providedServices": {
        "linter": {
            "versions": {
                "2.0.0": "provideLinter"
            }
        }
    }
}
