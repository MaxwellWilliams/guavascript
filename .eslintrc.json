{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "consistent-return": 0,
        "no-plusplus": 0,
        "no-param-reassign": 0,
        "no-lonely-if": 0,
        "no-restricted-syntax": 0,
        "no-prototype-builtins": 0,
        "no-restricted-properties": 0,
        "guard-for-in": 0,
        "global-require": 0,
        "import/no-dynamic-require": 0,
        "class-methods-use-this": 0
    },
    "globals": {
      "describe": true,
      "it": true
    }
}
